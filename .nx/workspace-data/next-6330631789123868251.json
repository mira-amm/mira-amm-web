{
  "1806666102197596995": {
    "build": {
      "command": "next build",
      "options": {
        "cwd": ".",
        "tty": false
      },
      "dependsOn": [
        "^build"
      ],
      "cache": true,
      "inputs": [
        "default",
        "^default",
        {
          "externalDependencies": [
            "next"
          ]
        }
      ],
      "outputs": [
        "{projectRoot}/.next",
        "{projectRoot}/.next/!(cache)"
      ]
    },
    "next:dev": {
      "command": "next dev",
      "options": {
        "cwd": "."
      }
    },
    "next:start": {
      "command": "next start",
      "options": {
        "cwd": "."
      },
      "dependsOn": [
        "build"
      ]
    },
    "serve-static": {
      "command": "next start",
      "options": {
        "cwd": "."
      },
      "dependsOn": [
        "build"
      ]
    },
    "build-deps": {
      "dependsOn": [
        "^build"
      ]
    },
    "watch-deps": {
      "dependsOn": [
        "build-deps"
      ],
      "command": "pnpm exec nx watch --projects mira-amm --includeDependentProjects -- pnpm exec nx build-deps mira-amm"
    }
  }
}