{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "outputDirectory": "api",
  "installCommand": "pnpm i",
  "buildCommand": "pnpm nx build api",
  "redirects": [
    {
      "source": "/",
      "destination": "/docs/scalar"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ]
}
