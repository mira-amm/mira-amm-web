  "scripts": {
    "dev": "nx dev:tui microvisor; process-compose down; zellij ka -y; zellij da -y;",
    "start": "nx dev microvisor --outputStyle=tui; process-compose down; zellij ka -y; zellij da -y;",
    "clean": "nx destroy db; git clean -fdX -e '!**/.env*'; # ðŸ§½ Remove any and all files listed in .gitignore except .env",
    "nuke": "nx destroy db; sudo git clean -fdX; # ðŸš¨ WARNING: REMOVE ANY AND ALL FILES LISTED IN .gitignore INCLUDING .env",
    "ri": "pnpm clean; pnpm i; pnpm d; # ðŸ§¹ Perform a clean install",
    "d": "nx shell microdoctor # ðŸ’Š Get a check-up from Microdoctor",
    "storybook": "nx storybook design -p $STORYBOOK_DEV_SERVER_PORT # ðŸŽ¨ Design System - Storybook",
    "fix:formatting": "pnpx prettier --write apps/web",
    "commit": "cz #ðŸŸ¢ Commitizen CLI",
    "postinstall": "nx postinstall microvisor"
  },
