  "nx": {
    "includedScripts": [],
    "targets": {
      "local-registry": {
        "executor": "@nx/js:verdaccio",
        "options": {
          "port": 4873,
          "config": ".verdaccio/config.yml",
          "storage": "tmp/local-registry/storage"
        }
      },
      "format:rust": {
        "executor": "nx:run-commands",
        "options": {
          "command": "find . -name '*.rs' -not -path './target/*' -not -path './node_modules/*' -not -path './.next/*' | xargs rustfmt --edition 2021"
        }
      },
      "format:rust:check": {
        "executor": "nx:run-commands",
        "options": {
          "command": "find . -name '*.rs' -not -path './target/*' -not -path './node_modules/*' -not -path './.next/*' | xargs rustfmt --edition 2021 --check"
        }
      },
      "format:all": {
        "executor": "nx:run-commands",
        "options": {
          "commands": [
            "nx format:write --all",
            "nx format:rust"
          ],
          "parallel": false
        }
      },
      "format:all:check": {
        "executor": "nx:run-commands",
        "options": {
          "commands": [
            "nx format:check --all",
            "nx format:rust:check"
          ],
          "parallel": false
        }
      }
    }
  },
